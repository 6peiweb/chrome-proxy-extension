{
    "manifest_version": 2,
    "name": "Chrome 代理",
    "version": "1.0.0",
    "description": "Chrome Proxy Extension",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "browser_action": {
        "default_icon": "wait.png",
        "default_title": "Chrome 代理",
        "default_popup": "./src/popup/index.html"
    },
    "permissions": [
        "contextMenus",
        "tabs",
        "notifications",
        "webRequest",
        "webRequestBlocking",
        "storage",
        "http://*/*",
        "https://*/*"
    ]
}